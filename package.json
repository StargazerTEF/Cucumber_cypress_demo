{
  "name": "cucumber_cypress_demo",
  "version": "1.0.0",
  "description": "Cucumber cypress demo",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "booking_stays": "cypress run --headed --spec cypress/e2e/step_definitions/booking_stays.feature --reporter mochawesome",
    "booking_flights": "cypress run --headed --spec cypress/e2e/step_definitions/booking_flights.feature --reporter mochawesome"
  },
  "author": "StargazerTEF",
  "license": "ISC",
  "dependencies": {
    "cypress": "^13.6.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "mochawesome": "^7.1.3"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step_definitions/"
  }
}
